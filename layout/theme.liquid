    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="{{ 'global.css' |  asset_url }}">
    <link rel="stylesheet" href="{{ 'tailwind.css' |  asset_url }}">
    <link rel="stylesheet" href="{{ 'header.css' |  asset_url }}">
   

    {%- if settings.favicon != blank -%}
        <link rel="icon" type="image/png" href="{{ settings.favicon | image_url: width: 35, height: 35 }}">
    {%- endif -%}

    <title>VapeSter</title>

    {{ content_for_header }}

    <style>

        :root {
            --page-width: {{ settings.page_width }}px;
            --logo-width: {{ settings.logo_width }}px;
            --bp: 960px;
            }     

    </style>



    <script>
      console.log({{settings.logo_width}})
    </script>


    
</head>
<body>
    {% section "header" %}

     <main id="MainContent" class="content-for-layout focus-none" role="main" tabindex="-1">
      {{ content_for_layout }}
    </main>
</body>
</html>




